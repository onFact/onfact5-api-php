{
    "name": "onfact/onfact5-api-php",
    "type": "standard",
    "description": "Standard for PHP package skeletons.",
    "homepage": "https://github.com/php-pds/skeleton",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "OnFact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnFact\\": "tests/"
        }
    },
    "bin": ["bin/pds-skeleton"],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.4.1",
        "doctrine/inflector": "^1.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vlucas/phpdotenv": "^4.1"
    }
}
